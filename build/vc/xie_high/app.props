    </BuildMacro>
    <BuildMacro Include="XIE_LIB">
      <Value>$(XIE_LIB)</Value>
    </BuildMacro>
    <BuildMacro Include="CUDA_INC">
      <Value>$(CUDA_INC)</Value>
    </BuildMacro>
    <BuildMacro Include="CUDA_LIB">
      <Value>$(CUDA_LIB)</Value>
    </BuildMacro>
    <BuildMacro Include="INTERNAL_HEADERS">
      <Value>$(INTERNAL_HEADERS)</Value>
    </BuildMacro>
    <BuildMacro Include="FILE_HEADERS">
      <Value>$(FILE_HEADERS)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>